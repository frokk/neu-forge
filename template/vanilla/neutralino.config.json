{
	"applicationId": "js.neutralino.vanilla",
	"version": "1.0.0",
	"defaultMode": "window",
	"port": 0,
	"documentRoot": "/www/",
	"url": "/",
	"enableServer": true,
	"enableNativeAPI": true,
	"tokenSecurity": "one-time",
	"logging": {
		"enabled": true,
		"writeToLogFile": true
	},
	"nativeAllowList": [
		"app.*",
		"os.*",
		"debug.log"
	],
	"globalVariables": {},
	"modes": {
		"window": {
			"title": "Neutralinojs Vanilla App",
			"width": 800,
			"height": 500,
			"minWidth": 400,
			"minHeight": 200,
			"fullScreen": false,
			"alwaysOnTop": false,
			"icon": "/www/icons/appIcon.png",
			"enableInspector": true,
			"borderless": false,
			"maximize": false,
			"hidden": false,
			"resizable": true,
			"exitProcessOnClose": true
		}
	},
	"cli": {
		"binaryName": "neutralino-vanilla",
		"resourcesPath": "/www/",
		"extensionsPath": "/extensions/",
		"clientLibrary": "/www/js/neutralino.js",
		"binaryVersion": "4.6.0",
		"clientVersion": "3.5.0"
	}
}
